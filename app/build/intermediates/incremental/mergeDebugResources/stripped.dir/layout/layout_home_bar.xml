<?xml version="1.0" encoding="utf-8"?>

                                                       
                                                   

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_blue_0050B4"
        android:orientation="vertical" android:tag="layout/layout_home_bar_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

        <com.example.mvvmlibrary.customizeview.StatusBarView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:elevation="1dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/home_logo_tv"
                android:layout_width="1dp"
                android:layout_height="30dp"
                android:layout_marginStart="16.5dp"
                android:src="@mipmap/ic_launcher"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="主页面"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/home_logo_tv"
                app:layout_constraintLeft_toRightOf="@id/home_logo_tv"
                app:layout_constraintTop_toTopOf="@id/home_logo_tv" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
         