<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="activity_zoom_view_vm"
            type="com.guyuan.dear.focus.contract.view.zoomView.ZoomViewViewModel" />
    </data>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.guyuan.dear.focus.contract.view.zoomView.ZoomViewActivity">

        <include layout="@layout/toolbar"/>

        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/activity_zoom_view_vp"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>