<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/item_staff_attend_list_normal_anchor_view_5"
        android:layout_width="1dp"
        android:layout_height="@dimen/dp_25"
        android:layout_marginLeft="@dimen/dp_50"
        android:background="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_staff_attend_list_normal_tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_5"
        android:layout_marginLeft="@dimen/dp_40"
        android:textSize="@dimen/sp_19"
        android:textStyle="bold"
        android:text="xx年xx月xx日"/>

    <View
        android:id="@+id/item_staff_attend_list_normal_anchor_view_1"
        android:layout_width="1dp"
        android:layout_height="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_50"
        android:background="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_staff_attend_list_normal_tv_date"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_staff_attend_list_normal_anchor_view_2"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        app:srcCompat="@drawable/ic_svg_round"
        app:layout_constraintTop_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_1"
        app:layout_constraintLeft_toLeftOf="@id/item_staff_attend_list_normal_anchor_view_1"
        app:layout_constraintRight_toRightOf="@id/item_staff_attend_list_normal_anchor_view_1"
        />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_staff_attend_list_normal_anchor_tv_work_start_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/item_staff_attend_list_normal_anchor_view_2"
        app:layout_constraintBottom_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_2"
        app:layout_constraintTop_toTopOf="@id/item_staff_attend_list_normal_anchor_view_2"
        android:text="上班时间："/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_staff_attend_list_normal_tv_work_start_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/item_staff_attend_list_normal_anchor_tv_work_start_time"
        app:layout_constraintBottom_toBottomOf="@id/item_staff_attend_list_normal_anchor_tv_work_start_time"
        app:layout_constraintTop_toTopOf="@id/item_staff_attend_list_normal_anchor_tv_work_start_time"/>



    <View
        android:id="@+id/item_staff_attend_list_normal_anchor_view_3"
        android:layout_width="1dp"
        android:layout_height="@dimen/dp_10"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_2"
        app:layout_constraintLeft_toLeftOf="@id/item_staff_attend_list_normal_anchor_view_2"
        app:layout_constraintRight_toRightOf="@id/item_staff_attend_list_normal_anchor_view_2"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_staff_attend_list_normal_anchor_view_4"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        app:srcCompat="@drawable/ic_svg_round"
        app:layout_constraintTop_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_3"
        app:layout_constraintLeft_toLeftOf="@id/item_staff_attend_list_normal_anchor_view_3"
        app:layout_constraintRight_toRightOf="@id/item_staff_attend_list_normal_anchor_view_3"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_staff_attend_list_normal_anchor_tv_work_end_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/item_staff_attend_list_normal_anchor_view_4"
        app:layout_constraintBottom_toBottomOf="@id/item_staff_attend_list_normal_anchor_view_4"
        app:layout_constraintTop_toTopOf="@id/item_staff_attend_list_normal_anchor_view_4"
        android:text="下班时间："/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_staff_attend_list_normal_tv_work_end_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/item_staff_attend_list_normal_anchor_tv_work_end_time"
        app:layout_constraintBottom_toBottomOf="@id/item_staff_attend_list_normal_anchor_tv_work_end_time"
        app:layout_constraintTop_toTopOf="@id/item_staff_attend_list_normal_anchor_tv_work_end_time" />


</androidx.constraintlayout.widget.ConstraintLayout>