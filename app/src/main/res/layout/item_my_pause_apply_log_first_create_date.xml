<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="data"
            type="com.guyuan.dear.work.contractPause.beans.myPauseApplyDetail.ApproveLogFirstCreateDate" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/item_my_pause_apply_log_first_create_date_anchor_view_1"
            android:layout_width="@dimen/dp_24"
            android:layout_height="@dimen/dp_24"
            app:srcCompat="@drawable/ic_svg_timer"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_my_pause_apply_log_first_create_date_anchor_view_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_black_333333"
            android:textStyle="bold"
            android:text="申请人："
            android:layout_marginLeft="@dimen/dp_4"
            app:layout_constraintTop_toTopOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_1"
            app:layout_constraintBottom_toBottomOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_1"
            app:layout_constraintLeft_toRightOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_1"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_black_333333"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_2"
            app:layout_constraintLeft_toRightOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_2"
            android:text="@{data.applier}"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:textColor="@color/color_grey_999999"
            android:textSize="@dimen/sp_14"
            android:layout_marginBottom="@dimen/dp_6"
            app:layout_constraintLeft_toLeftOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_2"
            app:layout_constraintTop_toBottomOf="@id/item_my_pause_apply_log_first_create_date_anchor_view_2"
            app:layout_constraintBottom_toBottomOf="parent"
            android:text="xxxxxx"
            app:setContractDate="@{data.createDate}"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>