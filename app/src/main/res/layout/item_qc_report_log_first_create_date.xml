<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="data"
            type="com.guyuan.dear.focus.qc.beans.verfifyLog.LogTypeFirstCreateDate" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/item_qc_report_log_first_create_date_anchor_view_1"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_svg_timer"
            android:layout_marginLeft="@dimen/dp_8"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_qc_report_log_first_create_date_anchor_view_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:text="首次创建"
            android:textColor="@color/color_black_333333"
            android:textSize="@dimen/sp_14"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@id/item_qc_report_log_first_create_date_anchor_view_1"
            app:layout_constraintLeft_toRightOf="@id/item_qc_report_log_first_create_date_anchor_view_1"
            app:layout_constraintTop_toTopOf="@id/item_qc_report_log_first_create_date_anchor_view_1" />


        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:layout_marginBottom="@dimen/dp_6"
            android:text="xxxxxx"
            android:textColor="@color/color_grey_999999"
            android:textSize="@dimen/sp_14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/item_qc_report_log_first_create_date_anchor_view_2"
            app:layout_constraintTop_toBottomOf="@id/item_qc_report_log_first_create_date_anchor_view_2"
            app:setContractDate="@{data.date}" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>